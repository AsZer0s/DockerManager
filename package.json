{
  "name": "docker-manager",
  "version": "0.63.1",
  "description": "Docker 管理系统",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "all": "concurrently \"npm run init\" \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "quick-start": "docker-compose up -d",
    "quick-stop": "docker-compose down",
    "quick-restart": "docker-compose restart",
    "quick-logs": "docker-compose logs -f",
    "quick-status": "docker-compose ps",
    "setup": "node scripts/setup.js",
    "security-check": "node scripts/security-check.js",
    "test-ssh": "node scripts/test-ssh-optimization.js",
    "lint": "cd client && npm run lint",
    "lint:fix": "cd client && npm run lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "scripts/deploy.sh",
    "deploy:prod": "scripts/deploy.sh deploy prod",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "backup": "scripts/deploy.sh backup",
    "health": "curl -f http://localhost:3000/health || echo 'Service not running'"
  },
  "keywords": ["docker", "telegram", "bot", "web", "management"],
  "author": "Zer0Teams",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.4.2"
  }
}
