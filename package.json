{
  "name": "docker-manager",
  "version": "0.58.1",
  "description": "Docker 管理系统",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "all": "concurrently \"npm run init\" \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": ["docker", "telegram", "bot", "web", "management"],
  "author": "Zer0Teams",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
